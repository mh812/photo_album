<AnchorPane prefHeight="223.0" prefWidth="384.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.PhotoDetailsController">
   <children>
      <GridPane layoutX="31.0" layoutY="25.0" prefHeight="64.0" prefWidth="322.0">
