        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Caption:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <TextField fx:id="caption" promptText="Caption Text" GridPane.columnIndex="1">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tags:" GridPane.rowIndex="1">
